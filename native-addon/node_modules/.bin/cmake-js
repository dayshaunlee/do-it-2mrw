#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Dayshaun/source/repos/do-it-2mrw/native-addon/node_modules/.pnpm/cmake-js@7.3.1/node_modules/cmake-js/bin/node_modules:/mnt/c/Users/Dayshaun/source/repos/do-it-2mrw/native-addon/node_modules/.pnpm/cmake-js@7.3.1/node_modules/cmake-js/node_modules:/mnt/c/Users/Dayshaun/source/repos/do-it-2mrw/native-addon/node_modules/.pnpm/cmake-js@7.3.1/node_modules:/mnt/c/Users/Dayshaun/source/repos/do-it-2mrw/native-addon/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Dayshaun/source/repos/do-it-2mrw/native-addon/node_modules/.pnpm/cmake-js@7.3.1/node_modules/cmake-js/bin/node_modules:/mnt/c/Users/Dayshaun/source/repos/do-it-2mrw/native-addon/node_modules/.pnpm/cmake-js@7.3.1/node_modules/cmake-js/node_modules:/mnt/c/Users/Dayshaun/source/repos/do-it-2mrw/native-addon/node_modules/.pnpm/cmake-js@7.3.1/node_modules:/mnt/c/Users/Dayshaun/source/repos/do-it-2mrw/native-addon/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/cmake-js@7.3.1/node_modules/cmake-js/bin/cmake-js" "$@"
else
  exec node  "$basedir/../.pnpm/cmake-js@7.3.1/node_modules/cmake-js/bin/cmake-js" "$@"
fi
